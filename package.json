{
  "name": "simple-vue-vueify-vue-router-setup",
  "version": "1.0.0",
  "description": "sample project to see a project setup with vue modules and an spa approach",
  "main": "build.js",
  "scripts": {
    "dev":"budo main.js:build.js --live --open -- -p browserify-hmr",
    "build":"browserify main.js -o build.js"
  },
  "keywords": [
    "vue",
    "vue-router",
    "browserify",
    "vueify",
    "budo"
  ],
  "author": "Leonardo Silveira <sombriks@gmail.com>",
  "license": "BSD-2-Clause",
  "dependencies": {
    "vue": "^2.1.3",
    "vue-router": "^2.0.3"
  },
  "devDependencies": {
    "browserify": "^13.1.1",
    "browserify-hmr": "^0.3.5",
    "budo": "^9.2.2",
    "vueify": "^9.3.0"
  },
  "browserify":{
    "transform":[
      "vueify"
    ]
  }
}
